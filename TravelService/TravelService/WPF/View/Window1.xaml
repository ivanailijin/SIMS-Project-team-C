<Window x:Class="TravelService.WPF.View.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Width="300" Height="50" Click="MostVisited_Click">Most Visited</Button>
            <Button Width="300" Height="50" Click="PastTours_Click">Tour in Past</Button>
            <Button Width="300" Height="50"  Click="TourOverview_Click">Active today</Button>
            <Button Width="300" Height="50" Click="ThisWeekTours_Click">My Tours</Button>
            <Button Width="300" Height="50" Click="AddTourView_CLick">Add Tour</Button>
        </StackPanel>

    </Grid>
</Window>
