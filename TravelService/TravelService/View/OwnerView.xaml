        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravelService.View"
        mc:Ignorable="d"
        Title="OwnerView" WindowStartupLocation="CenterScreen" Height="450" Width="800">
    <Grid>

        <Grid.RowDefinitions>
