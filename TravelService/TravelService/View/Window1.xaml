        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravelService.View"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Button Width="300" Height="50" Click="AddTourView_CLick">Add Tour view</Button>
        <Button Width="300" Height="50"  Margin="0,150,0,0" Click="TourOverview_Click">Tour Overview</Button>
        <Button Width="300" Height="50"  Margin="0,280,0,0" Click="ThisWeekTours_Click">My tours </Button>
    </Grid>
</Window>